{
  "name": "project-sesame",
  "version": "1.0.0",
  "description": "Project Sesame",
  "main": "server.mjs",
  "type": "module",
  "scripts": {
    "build": "npm run clean; webpack",
    "clean": "rimraf ./dist",
    "dev": "NODE_ENV=localhost node dist/server.mjs",
    "start": "node dist/server.mjs",
    "deploy": "npm run build && gcloud app deploy dev.yaml --project=google.com:project-sesame",
    "deploy:prod": "npm run build && gcloud app deploy app.yaml --project=google.com:project-sesame",
    "logs": "gcloud app logs tail --project=google.com:project-sesame",
    "emulator": "firebase emulators:start --only firestore --project=google.com:project-sesame --import=./.data --export-on-exit"
  },
  "dependencies": {
    "@google-cloud/connect-firestore": "3.0.0",
    "@material/mwc-drawer": "0.27.0",
    "@material/mwc-snackbar": "0.27.0",
    "@material/mwc-top-app-bar": "0.27.0",
    "@material/textfield": "^14.0.0",
    "@material/web": "1.2.0",
    "@simplewebauthn/server": "9.0.1",
    "@simplewebauthn/types": "9.0.1",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.16",
    "base64url": "3.0.1",
    "dotenv": "16.4.1",
    "express": "4.18.2",
    "express-handlebars": "7.1.2",
    "express-session": "1.18.0",
    "express-useragent": "1.0.15",
    "firebase-admin": "12.0.0",
    "jsonwebtoken": "9.0.2",
    "lit": "3.1.2"
  },
  "engines": {
    "node": ">=18.x"
  },
  "repository": {
    "url": "https://github.com/GoogleChromeLabs/passkeys-demo"
  },
  "license": "Apache-2.0",
  "keywords": [
    "node",
    "express",
    "webauthn",
    "fido2",
    "passkey"
  ],
  "devDependencies": {
    "@babel/core": "7.23.9",
    "@types/express": "^4.17.21",
    "@types/express-handlebars": "^6.0.0",
    "@types/express-session": "^1.17.10",
    "@types/express-useragent": "^1.0.5",
    "babel-loader": "9.1.3",
    "babel-preset-env": "1.7.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "6.10.0",
    "extract-loader": "5.1.0",
    "file-loader": "6.2.0",
    "firebase-tools": "13.1.0",
    "glob": "^10.3.10",
    "rimraf": "^5.0.5",
    "sass": "1.70.0",
    "sass-loader": "14.1.0",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "5.90.1",
    "webpack-cli": "5.1.4"
  }
}
