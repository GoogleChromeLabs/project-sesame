{
  "name": "project-sesame",
  "version": "1.0.0",
  "description": "Project Sesame",
  "main": "server.mjs",
  "type": "module",
  "scripts": {
    "build": "webpack",
    "dev": "NODE_ENV=localhost node server.mjs",
    "start": "node server.mjs",
    "deploy": "npm run build && gcloud app deploy dev.yaml --project=google.com:project-sesame",
    "deploy:prod": "npm run build && gcloud app deploy app.yaml --project=google.com:project-sesame",
    "logs": "gcloud app logs tail --project=google.com:project-sesame",
    "emulator": "firebase emulators:start --only firestore --project=google.com:project-sesame --import=./.data --export-on-exit"
  },
  "dependencies": {
    "@google-cloud/connect-firestore": "^3.0.0",
    "@material/mwc-button": "^0.27.0",
    "@material/mwc-drawer": "^0.27.0",
    "@material/mwc-fab": "^0.27.0",
    "@material/mwc-icon-button": "^0.27.0",
    "@material/mwc-linear-progress": "^0.27.0",
    "@material/mwc-list": "^0.27.0",
    "@material/mwc-textfield": "^0.27.0",
    "@material/mwc-top-app-bar": "^0.27.0",
    "@material/web": "^1.1.1",
    "@simplewebauthn/server": "^8.3.5",
    "base64url": "^3.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-handlebars": "^7.1.2",
    "express-session": "^1.17.3",
    "express-useragent": "^1.0.15",
    "firebase-admin": "^12.0.0",
    "lit": "^3.1.0",
    "material-components-web": "^14.0.0"
  },
  "engines": {
    "node": ">=18.x"
  },
  "repository": {
    "url": "https://github.com/GoogleChromeLabs/passkeys-demo"
  },
  "license": "Apache-2.0",
  "keywords": [
    "node",
    "express",
    "webauthn",
    "fido2",
    "passkey"
  ],
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "babel-loader": "^9.1.3",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^6.8.1",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.2.0",
    "firebase-tools": "^13.6.0",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
