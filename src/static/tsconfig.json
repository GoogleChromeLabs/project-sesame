{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "lib": [ "ESNext", "DOM" ],
      "outDir": "./dist",
      "strict": true,
      "allowJs": true,
      "checkJs": true,
      "noUnusedLocals": true,
      "noImplicitReturns": true,
      "noImplicitThis": false,
      "moduleResolution": "Bundler",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true
    }
  }
  